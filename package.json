{
  "name": "@helicone/generate",
  "version": "1.0.2",
  "description": "A light and modern interface for Helicone's generateRouter API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --environment node",
    "test:watch": "vitest --environment node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Helicone/generate.git"
  },
  "bugs": {
    "url": "https://github.com/Helicone/helicone/issues"
  },
  "homepage": "https://helicone.ai",
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "prompt",
    "llm",
    "helicone",
    "api",
    "ai",
    "generate"
  ],
  "author": "Helicone",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.24",
    "dotenv": "^16.4.7",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
